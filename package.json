{
  "name": "p2-implementation",
  "version": "1.0.0",
  "scripts": {
    "start": "turbo start",
    "dev": "turbo dev",
    "build": "turbo build",
    "style": "biome check",
    "style:fix": "biome check --fix",
    "style:fix:unsafe": "biome check --fix --unsafe",
    "db:migrate": "pnpm --filter @repo/database run migrate",
    "db:generate": "pnpm --filter @repo/database run generate",
    "db:push": "pnpm --filter @repo/database run push"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@repo/typescript-config": "workspace:*",
    "turbo": "^2.2.3",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@9.12.3"
}
